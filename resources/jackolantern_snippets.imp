@@@contains(list, item)
if (item != null && list.contains(item) == false) {
	list += item
}
@@@equals(first, second)
first = second
@@@map_on_add(T, list)
if (list != null) {
	var item:T = new T()
	item.initialize()
	list.add(item)
}
@@@map_on_add_with_hub(T, list, hub)
if (list != null) {
	var item:T = new T()
	item.initialize(hub)
	list.add(item)
}